{
  "name": "nuxeouploader",
  "productName": "FileUploader",
  "description": "Starter for your Electron application",
  "version": "0.1.8",
  "author": "Brian Tingle",
  "copyright": "Â© 2018, Regents of the University of California",
  "homepage": "http://example.com",
  "license": "BSD 3-clause",
  "main": "app/background.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ucldc/nuxeo_uploader.git"
  },
  "build": {
    "appId": "org.cdlib.nuxeouploader",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "build": "gulp build",
    "prerelease": "gulp build --env=production",
    "release": "build -w -m",
    "start": "gulp start",
    "pretest": "gulp build-unit --env=test",
    "test": "electron-mocha app/specs.js.autogenerated --renderer --require source-map-support/register",
    "coverage": "npm test -- -R scripts/istanbul-reporter",
    "pree2e": "gulp build-e2e --env=test",
    "e2e": "mocha app/e2e.js.autogenerated --require source-map-support/register",
    "dist": "electron-builder -w -m"
  },
  "dependencies": {
    "backbone": "^1.3.3",
    "backbone.epoxy": "^1.3.4",
    "backbone.localstorage": "^1.1.16",
    "backbone.marionette": "^2.3.2",
    "bluebird": "^3.5.0",
    "bootstrap": "^3.3.7",
    "check_checksum": "^1.0.0",
    "filesize": "^3.5.6",
    "fs-jetpack": "^0.10.2",
    "jquery": "^3.1.1",
    "jstree": "^3.3.3",
    "md5-file": "^3.1.1",
    "nuxeo": "^3.0.1",
    "package-lock": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "electron": "^22.3.25",
    "electron-builder": "^20.28.2",
    "electron-mocha": "^6.0.4",
    "gulp": "^3.9.1",
    "gulp-batch": "^1.0.5",
    "gulp-less": "^3.0.3",
    "gulp-plumber": "^1.1.0",
    "gulp-util": "^3.0.6",
    "gulp-watch": "^4.3.5",
    "istanbul": "^0.4.3",
    "minimist": "^1.2.0",
    "mocha": "^5.2.0",
    "rollup": "^0.36.3",
    "rollup-plugin-istanbul": "^1.1.0",
    "source-map-support": "^0.4.2",
    "spectron": "^3.3.0"
  }
}
